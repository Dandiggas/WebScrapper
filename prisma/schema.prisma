generator client {
  provider = "prisma-client-py"
}


datasource db {
  provider = "postgresql"
  url      = "postgresql://dandiggas:dandiggas@localhost:5432/dandiggas"
}

model User {
  id    Int    @id @default(autoincrement())
  name  String
  price String
}